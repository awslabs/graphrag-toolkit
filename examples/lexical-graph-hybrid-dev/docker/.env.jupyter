# Local Docker Environment Configuration
# Based on docker-compose.yml services

# Local Graph Database (FalkorDB)
GRAPH_STORE=falkordb://falkordb:6379
VECTOR_STORE=pgvector://postgres:password@pgvector-db:5432/graphrag
FALKORDB_HOST=falkordb
FALKORDB_PORT=6379

# FalkorDB Browser
FALKORDB_BROWSER_URL=http://localhost:8092

# Local Testing S3 Bucket
LOCAL_EXTRACT_S3=ccms-document-graph
PROMPT_S3=ccms-document-graph

# AWS Configuration (for testing)
AWS_PROFILE=master
AWS_REGION=us-east-1

# Bedrock Models (for AWS testing)
EXTRACTION_MODEL=us.anthropic.claude-3-5-sonnet-20240620-v1:0
RESPONSE_MODEL=us.anthropic.claude-3-5-sonnet-20240620-v1:0
EVALUATION_MODEL=us.anthropic.claude-3-5-sonnet-20240620-v1:0
EMBEDDINGS_MODEL=cohere.embed-english-v3
EMBEDDINGS_DIMENSIONS=1024

# PostgreSQL Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password
POSTGRES_DB=graphrag

# Application Configuration
APPLICATION_ID=lexical-graph-hybrid-dev